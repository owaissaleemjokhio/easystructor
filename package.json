{
  "name": "easystructor",
  "displayName": "EasyStructor",
  "description": "Easystructor is a powerful VS Code Extension that provides a complete, end-to-end toolkit for Laravel developers.",
  "version": "1.1.2",
  "publisher": "easystructor",
  "engines": {
    "vscode": "^1.102.0"
  },
  "icon": "logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/owaissaleemjokhio/easystructor"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "keywords": [
    "Scaffold",
    "Boilerplate",
    "Generator",
    "Project Management",
    "Laravel",
    "PHP",
    "CRUD"
  ],
  "activationEvents": [
    "onCommand:easystructor.openModal",
    "onCommand:easystructor.generateModule",
    "onCommand:easystructor.laravel.generateCrud",
    "onCommand:easystructor.laravel.revertCrud"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "easystructor",
          "title": "Easystructor",
          "icon": "logo.png"
        }
      ]
    },
    "views": {
      "easystructor": [
        {
          "id": "easystructorView",
          "name": "Dashboard"
        }
      ]
    },
    "commands": [
      {
        "command": "easystructor.generateModule",
        "title": "Easystructor: Generate Module"
      },
      {
        "command": "easystructor.openSettings",
        "title": "Easystructor: Open Settings"
      },
      {
        "command": "easystructor.openAbout",
        "title": "Easystructor: About"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run compile-less",
    "compile": "tsc -p ./",
    "compile-less": "lessc src/frameworks/laravel/webview/modal.less src/frameworks/laravel/webview/modal.css",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/pluralize": "^0.0.33",
    "@types/vscode": "^1.102.0",
    "less": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "pluralize": "^8.0.0"
  }
}