{
  "name": "easystructor",
  "displayName": "EasyStructor",
  "description": "Generate clean modular code structures (CRUD, services, resources) for Laravel, NestJS, and more.",
  "version": "1.0.1",
  "publisher": "easystructor",
  "engines": {
    "vscode": "^1.102.0"
  },
  "icon": "logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/owaissaleemjokhio/easystructor"
  },
  "categories": [
    "Laravel",
    "PHP",
    "JavaScript",
    "TypeScript",
    "Other",
    "Snippets",
    "Code Generators",
    "Frameworks"
  ],
  "activationEvents": [
    "onCommand:easystructor.openModal",
    "onCommand:easystructor.generateModule",
    "onCommand:easystructor.laravel.generateCrud",
    "onCommand:easystructor.laravel.revertCrud"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "easystructor.generateModule",
        "title": "Easystructor: Generate Module"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run compile-less",
    "compile": "tsc -p ./",
    "compile-less": "lessc src/webview/modal.less src/webview/modal.css",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/pluralize": "^0.0.33",
    "@types/vscode": "^1.102.0",
    "less": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "pluralize": "^8.0.0"
  }
}